<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="qhacemosVM" type="com.application.app.modules.qhacemos.data.viewmodel.QhacemosVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnmask"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_2">
        <LinearLayout android:id="@+id/linearQhacemos"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackbamxlogotwo"
                android:layout_height="@dimen/_56pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageBamxlogoTwo"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_64pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_62pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_bamxlogo2_18}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_bamxlogo2_18"/>
                <FrameLayout android:id="@+id/frameStackbamxlogotwoOne"
                    android:layout_gravity="start"
                    android:layout_height="70dp" android:layout_width="match_parent">
                    <FrameLayout android:id="@+id/frameStackarrowleft"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_70pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="match_parent" style="@style/groupStylebluegray_801">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_22pxh"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_width="@dimen/_26pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_white_a700"/>

                        <ImageView
                            android:id="@+id/imageBamxlogoTwoOne"
                            imageRes="@{@drawable/img_bamxlogo2_19}"
                            android:layout_width="@dimen/_62pxh"
                            android:layout_height="@dimen/_64pxh"
                            android:layout_gravity="bottom|end"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_bamxlogo2_19" />
                    </FrameLayout>

                </FrameLayout>
            </FrameLayout>

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <FrameLayout
                        android:layout_width = "fill_parent"
                        android:layout_height = "fill_parent"
                        android:orientation = "vertical">

                        <ImageView
                            android:id="@+id/imageEllipseFive"
                            imageRes="@{@drawable/img_ellipse5}"
                            android:layout_width="match_parent"
                            android:layout_height="200dp"
                            android:layout_gravity="start"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_ellipse5" />

                        <TextView
                            android:id="@+id/txtH8"
                            style="@style/txtRobotoromanextrabold25"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="40dp"
                            android:layout_marginStart="37dp"
                            android:layout_marginEnd="37dp"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="¿QUÉ HACEMOS?"
                            android:textColor="#FFFFFF"
                            android:textSize="35sp"
                            tools:text="@string/lbl_q_hacemos" />

                    </FrameLayout>

                    <ImageView
                        android:id="@+id/imageImageTwo"
                        imageRes="@{@drawable/img_image2}"
                        android:layout_width="@dimen/_285pxh"
                        android:layout_height="@dimen/_247pxh"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_marginEnd="@dimen/_37pxh"
                        android:scaleType="fitXY"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_image2" />

                    <TextView
                        android:id="@+id/txtH5"
                        style="@style/txtRobotoromanextrabold25"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_marginTop="@dimen/_38pxv"
                        android:layout_marginEnd="@dimen/_37pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="EN EL MUNDO"
                        tools:text="EN EL MUNDO" />

                    <TextView
                        android:id="@+id/txtDescription"
                        style="@style/txtRobotoromanregular15_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="37dp"
                        android:layout_marginTop="27dp"
                        android:layout_marginEnd="37dp"
                        android:gravity="center_horizontal"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:text="@{qhacemosVM.qhacemosModel.txtDescription}"
                        tools:text="@string/msg_se_desperdician" />

                    <ImageView
                        android:id="@+id/imageImageThree"
                        imageRes="@{@drawable/img_image3}"
                        android:layout_width="@dimen/_284pxh"
                        android:layout_height="@dimen/_200pxh"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_marginTop="@dimen/_48pxv"
                        android:layout_marginEnd="@dimen/_37pxh"
                        android:scaleType="fitXY"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_image3" />

                    <TextView
                        android:id="@+id/txtH6"
                        style="@style/txtRobotoromanextrabold25"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_marginTop="@dimen/_38pxv"
                        android:layout_marginEnd="@dimen/_37pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="EN MÉXICO"
                        android:textColor = "#439347"
                        tools:text="EN MÉXICO" />

                    <TextView
                        android:id="@+id/txtDescriptionOne"
                        style="@style/txtRobotoromanregular15_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="37dp"
                        android:layout_marginTop="27dp"
                        android:layout_marginEnd="25dp"
                        android:gravity="center_horizontal"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:text="@{qhacemosVM.qhacemosModel.txtDescriptionOne}"
                        tools:text="@string/msg_1_3_de_lo_que_s" />

                    <ImageView
                        android:id="@+id/imageImageSix"
                        imageRes="@{@drawable/img_image6}"
                        android:layout_width="144dp"
                        android:layout_height="186dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_marginTop="@dimen/_65pxv"
                        android:layout_marginEnd="@dimen/_37pxh"
                        android:scaleType="fitXY"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_image6" />

                    <TextView
                        android:id="@+id/txtH7"
                        style="@style/txtRobotoromanextrabold25"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_marginTop="@dimen/_38pxv"
                        android:layout_marginEnd="@dimen/_37pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="EN LA RED BAMX"
                        android:textColor="#D73946"
                        tools:text="EN LA RED BAMX" />

                    <TextView
                        android:id="@+id/txtWeight"
                        style="@style/txtRobotoromanregular15_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="37dp"
                        android:layout_marginTop="27dp"
                        android:layout_marginEnd="37dp"
                        android:layout_marginBottom = "10dp"
                        android:gravity="center_horizontal"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:text="@{qhacemosVM.qhacemosModel.txtWeight}"
                        tools:text="@string/msg_rescatamos_135" />

                    <FrameLayout
                        android:layout_width = "fill_parent"
                        android:layout_height = "fill_parent"
                        android:orientation = "vertical">
                        <ImageView
                            android:id="@+id/imageEllipseSix"
                            imageRes="@{@drawable/img_ellipse6}"
                            android:layout_width="match_parent"
                            android:layout_height="200dp"
                            android:layout_gravity="start"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_ellipse6" />
                    </FrameLayout>

                </LinearLayout>
            </ScrollView>

        </LinearLayout>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_1pxh" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linear2FourupAO"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylebluegray_802">
                <LinearLayout android:id="@+id/linear1Tab"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylebluegray_800">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="@dimen/_30pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtCaption"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.02"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{qhacemosVM.qhacemosModel.txtCaption}"
                        style="@style/txtRobotoregular12" tools:text="@string/lbl_men"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearColumnvolume"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylebluegray_800">
                    <LinearLayout android:id="@+id/linear3Tab"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylebluegray_800">
                        <ImageView android:id="@+id/imageVolume"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_volume"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtCaptionOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.02"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{qhacemosVM.qhacemosModel.txtCaptionOne}"
                            style="@style/txtRobotoregular12_1" tools:text="@string/lbl_information"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
