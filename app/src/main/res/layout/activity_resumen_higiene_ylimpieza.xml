
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable name="mensajeDonaciNVM" type="com.application.app.modules.mensajedonacin.data.viewmodel.MensajeDonaciNVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearMensajedonaci"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnmask"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameStackbamxlogotwo"
                android:layout_height="@dimen/_70pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageBamxlogoTwo"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_64pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_62pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_bamxlogo2_64x62}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_bamxlogo2_64x62"/>
                <FrameLayout android:id="@+id/frameStackbamxlogotwoOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_70pxv" android:layout_width="match_parent">

                    <FrameLayout android:id="@+id/frameStackarrowleft"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_56pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="match_parent" style="@style/groupStylebluegray_801">

                        <ImageView
                            android:id="@+id/imageArrowleft"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_22pxh"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_width="@dimen/_26pxh"
                            android:scaleType="fitXY"
                            android:focusable="true"
                            android:onClick="back"
                            android:src="@drawable/img_arrowleft_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_white_a700" />

                    </FrameLayout>
                    <ImageView android:id="@+id/imageBamxlogoTwoOne"
                        android:layout_gravity="bottom|end"
                        android:layout_height="@dimen/_64pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_62pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_bamxlogo2_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bamxlogo2_1"/>
                </FrameLayout>
            </FrameLayout>

            <TextView
                android:id="@+id/txtGraciasportu"
                style="@style/txtRobotoromansemibold30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_84pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_84pxh"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:text="Datos proporcionados"
                android:textAlignment="center"/>


            <FrameLayout
                android:id="@+id/frameAInactiveEna12"
                android:layout_width="@dimen/_363pxh"
                android:layout_height="@dimen/_56pxv"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_marginEnd="@dimen/_22pxh">

                <EditText
                    android:id="@+id/tipoHigiene"
                    style="@style/txtRobotoregular16_1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_10pxv"
                    android:textColor="#000000"
                    android:inputType="none"
                    android:letterSpacing="0.01"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:text="Categoría"
                    tools:text="Categoría"
                    />

                <ImageView
                    android:id="@+id/imageLabelFour22"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_49pxv"
                    android:layout_gravity="bottom|start"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_label"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_label" />

                <LinearLayout
                    android:id="@+id/linearLabel222"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|center"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:orientation="horizontal">


                    <View
                        android:id="@+id/lineUpperSurfaceTwo22"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_weight="0.71"
                        android:background="@color/black_900_1e" />
                </LinearLayout>
            </FrameLayout>


            <FrameLayout
                android:id="@+id/frameAInactiveEna1"
                android:layout_width="@dimen/_363pxh"
                android:layout_height="@dimen/_56pxv"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_marginEnd="@dimen/_22pxh">

                <EditText
                    android:id="@+id/cantHigiene"
                    style="@style/txtRobotoregular16_1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_10pxv"
                    android:textColor="#000000"
                    android:inputType="none"
                    android:letterSpacing="0.01"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:text="Cantidad"
                    tools:text="Cantidad"
                    />

                <ImageView
                    android:id="@+id/imageLabelFour"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_49pxv"
                    android:layout_gravity="bottom|start"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_label"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_label" />

                <LinearLayout
                    android:id="@+id/linearLabel2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|center"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:orientation="horizontal">


                    <View
                        android:id="@+id/lineUpperSurfaceTwo"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_weight="0.71"
                        android:background="@color/black_900_1e" />
                </LinearLayout>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/frameAInactiveEna2"
                android:layout_width="@dimen/_363pxh"
                android:layout_height="@dimen/_56pxv"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_marginEnd="@dimen/_22pxh">

                <EditText
                    android:id="@+id/nombreHigiene"
                    style="@style/txtRobotoregular16_1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_10pxv"
                    android:textColor="#000000"
                    android:inputType="none"
                    android:letterSpacing="0.01"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="Nombre de los productos"
                    tools:text="Nombre de los productos"
                    />

                <ImageView
                    android:id="@+id/imageLabelFour2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_49pxv"
                    android:layout_gravity="bottom|start"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_label"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_label" />

                <LinearLayout
                    android:id="@+id/linearLabel22"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|center"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:orientation="horizontal">


                    <View
                        android:id="@+id/lineUpperSurfaceTwo2"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_weight="0.71"
                        android:background="@color/black_900_1e" />
                </LinearLayout>
            </FrameLayout>



            <LinearLayout
                android:paddingTop="30dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_marginStart="60dp"
                android:layout_marginEnd="60dp"
                android:orientation="horizontal">


                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btn2"
                    style="@style/btnSolidRounded_2"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="false"
                    android:layout_weight="0.3"
                    android:onClick="back"
                    android:singleLine="true"
                    android:text="@string/lbl_correct"
                    android:textAllCaps="true"
                    tools:text="@string/lbl_correct"
                    />

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="wrap_content">

                </ImageView>




                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btn"
                    style="@style/btnSolidRounded_2"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.3"
                    android:background="@drawable/rectangle_bg_green_301_radius_10"
                    android:singleLine="true"
                    android:text="@string/lbl_enviar"
                    android:textAllCaps="true"
                    tools:text="@string/lbl_enviar" />
            </LinearLayout>












        </LinearLayout>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linear2FourupAO"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                style="@style/groupStylebluegray_802">

                <LinearLayout android:id="@+id/linear1Tab"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:orientation="vertical"
                    android:focusable="true"
                    android:onClick="returnMenu"
                    style="@style/groupStylebluegray_800">

                    <ImageView android:id="@+id/imageHome"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="@dimen/_30pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>

                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtCaption"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.02"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="Menú"
                        style="@style/txtRobotoregular12" tools:text="@string/lbl_men"/>
                </LinearLayout>

                <LinearLayout android:id="@+id/linearColumnvolume"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylebluegray_800"
                    android:focusable="true"
                    android:onClick="infoFromStripe">

                    <LinearLayout android:id="@+id/linear3Tab"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylebluegray_800">
                        <ImageView android:id="@+id/imageVolume"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_volume"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtCaptionOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.02"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="Información"
                            style="@style/txtRobotoregular12_1" tools:text="@string/lbl_information"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>






