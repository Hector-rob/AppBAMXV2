                                style="@style/etSolidRoundedOutline_1" tools:ignore="Autofill"/>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageBamxlogoTwoOne"
                            android:layout_gravity="bottom|end"
                            android:layout_height="@dimen/_64pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_62pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_bamxlogo2_23}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_bamxlogo2_23"/>
                    </FrameLayout>
                </FrameLayout>
            </FrameLayout>
            <LinearLayout android:gravity="center_horizontal"
