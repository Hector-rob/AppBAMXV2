                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginEnd="@dimen/_101pxh"
                        android:layout_marginStart="@dimen/_101pxh"
                        android:layout_marginTop="@dimen/_42pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{buscadorVM.buscadorModel.txtH5One}"
                        style="@style/txt_1" tools:text="@string/lbl_recetas2"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStackbamxlogotwo"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_70pxv"
                    android:layout_marginBottom="@dimen/_10pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageBamxlogoTwo"
                        android:layout_gravity="bottom|end"
                        android:layout_height="@dimen/_64pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_62pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_bamxlogo2_22}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bamxlogo2_22"/>
