                                    android:id="@+id/imageFavoriteThree"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_18pxh"
                                    android:layout_width="@dimen/_20pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_favorite"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_favorite"/>
                                    </FrameLayout>
                                </FrameLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </FrameLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnrecetacounterFive"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStyle_1">
                        <LinearLayout android:id="@+id/linearOverlay1"
                            android:layout_height="match_parent"
                            android:layout_marginTop="@dimen/_139pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" style="@style/groupStylewhite_A700">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtRecetaCounterFive"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_12pxv"
                                android:layout_marginStart="@dimen/_12pxh"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_weight="0.86"
                                android:layout_width="0dp"
                                android:letterSpacing="0.01"
                                android:lineSpacingExtra="@dimen/_8pxh"
                                android:singleLine="true"
                                android:text="@{buscadorVM.buscadorModel.txtRecetaCounterFive}"
                                style="@style/txtRobotoregular16" tools:text="@string/lbl_receta_6"/>
                            <ImageView
                                android:id="@+id/imageFavoriteFour"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_marginBottom="@dimen/_14pxv"
                                android:layout_marginEnd="@dimen/_14pxh"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_15pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_favorite"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_favorite"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
