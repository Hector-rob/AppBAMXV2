                            android:scaleType="fitXY"
                            android:src="@drawable/img_favorite"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_favorite"/>
                        <LinearLayout
                            android:id="@+id/linearColumnrecetacounterFour"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStyle_1">
                            <LinearLayout
                                android:id="@+id/linearOverlay"
                                android:layout_height="match_parent"
                                android:layout_marginTop="@dimen/_139pxv"
                                android:layout_width="match_parent"
                                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtRecetaCounterFour"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_12pxv"
                                    android:layout_marginStart="@dimen/_12pxh"
                                    android:layout_marginTop="@dimen/_12pxv"
                                    android:layout_weight="0.86"
                                    android:layout_width="0dp"
                                    android:letterSpacing="0.01"
                                    android:lineSpacingExtra="@dimen/_8pxh"
                                    android:singleLine="true"
                                    android:text="@{buscadorVM.buscadorModel.txtRecetaCounterFour}"
                                    style="@style/txtRobotoregular16" tools:text="@string/lbl_receta_5"/>
                                <FrameLayout
                                    android:id="@+id/frameStackfavoriteOne"
                                    android:layout_height="@dimen/_18pxv"
                                    android:layout_marginBottom="@dimen/_14pxv"
                                    android:layout_marginEnd="@dimen/_13pxh"
                                    android:layout_marginStart="@dimen/_10pxh"
                                    android:layout_marginTop="@dimen/_14pxv" android:layout_width="@dimen/_20pxh">
                                    <ImageView
